GetyProxyAddress/			#	获取IP代理池
	proxyaddress.py			#	爬虫，去各个代理网站获得代理

Storage/					#	存储器
    data.json               #   存储记录
	SocketServer.py			#	Socket服务端,模拟Web前端
	Storage.py				#	存储并返回数据,可以将数据存储到data.json中

Ver/				#	验证
	verification.py			#	验证代理是否可用

Logger/						#	日志
	logger.py				#	记录日志
	log/

SystemController		#	启动程序,调用对应的模块


